{
  "name": "captura",
  "version": "0.0.1",
  "description": "Next js tutorial",
  "repository": {
    "type": "git",
    "url": "git://github.com/aarryann/captura.git"
  },
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development MODE_ENV=local PORT=4812 ts-node --project tsconfig.server.json start.ts",
    "start": "next start",
    "test": "cross-env NODE_ENV=test MODE_ENV=local jest",
    "test:watch": "jest --watch",
    "transpile:server": "tsc --project tsconfig.server.json",
    "build:local": "cross-env NODE_ENV=production MODE_ENV=local next build && npm run transpile:server",
    "build:staging": "cross-env NODE_ENV=production MODE_ENV=staging next build && npm run transpile:server",
    "build:analyze": "cross-env ANALYZE=true NODE_ENV=production MODE_ENV=production next build && tsc --project tsconfig.server.json",
    "build:production": "cross-env NODE_ENV=production MODE_ENV=production next build && npm run transpile:server",
    "start:local": "cross-env NODE_ENV=production MODE_ENV=local PORT=443 NODE_EXTRA_CA_CERTS=./secrets/fullchain.cert.pem node dist/start.js --expose-http2 ",
    "start:staging": "cross-env NODE_ENV=production MODE_ENV=staging PORT=443 NODE_EXTRA_CA_CERTS=./secrets/fullchain.cert.pem node dist/start.js --expose-http2 ",
    "start:production": "cross-env NODE_ENV=production MODE_ENV=production PORT=443 NODE_EXTRA_CA_CERTS=./secrets/fullchain.cert.pem node dist/start.js --expose-http2 ",
    "lint": "eslint \"*/**/*.ts*\"",
    "eslint": "eslint . --ignore-path .gitignore",
    "eslint:fix": "eslint . --fix --ignore-path .gitignore",
    "gen:schema": "graphql-codegen"
  },
  "keywords": [],
  "author": "Vic Pil",
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@apollo/react-ssr": "^3.1.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@hapi/iron": "^6.0.0",
    "@zeit/next-sass": "^1.0.1",
    "apollo-boost": "^0.4.8",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.9",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.20",
    "apollo-server": "^2.13.0",
    "apollo-server-express": "^2.13.0",
    "apollo-utilities": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie": "^0.4.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-passport": "^0.6.3",
    "graphql-resolvers": "^0.4.2",
    "graphql-tag": "^2.10.3",
    "helmet": "^3.22.0",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "lodash.get": "^4.4.2",
    "next": "^9.3.6",
    "next-connect": "^0.6.6",
    "next-cookies": "^2.0.3",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.14.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.5",
    "swr": "^0.2.0",
    "ws": "^7.2.5"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.5",
    "@types/react": "^16.9.34",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.10.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "faker": "^4.1.0",
    "jest": "^26.0.1",
    "jest-watch-typeahead": "^0.6.0",
    "mocha": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120,
    "semi": true,
    "tabWidth": 2,
    "endOfLine": "auto"
  }
}
