{
  "name": "captura",
  "version": "0.0.1",
  "description": "Next js tutorial",
  "repository": {
    "type": "git",
    "url": "git://github.com/aarryann/captura.git"
  },
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development MODE_ENV=local PORT=4812 ts-node --project tsconfig.server.json start.ts",
    "start": "next start",
    "test": "cross-env NODE_ENV=test MODE_ENV=local jest",
    "test:watch": "jest --watch",
    "transpile:server": "tsc --project tsconfig.server.json",
    "build:local": "cross-env NODE_ENV=production MODE_ENV=local next build && npm run transpile:server",
    "build:staging": "cross-env NODE_ENV=production MODE_ENV=staging next build && npm run transpile:server",
    "build:analyze": "cross-env ANALYZE=true NODE_ENV=production MODE_ENV=production next build && tsc --project tsconfig.server.json",
    "build:production": "cross-env NODE_ENV=production MODE_ENV=production next build && npm run transpile:server",
    "start:local": "cross-env NODE_ENV=production MODE_ENV=local PORT=443 NODE_EXTRA_CA_CERTS=./secrets/fullchain.cert.pem node dist/start.js --expose-http2 ",
    "start:staging": "cross-env NODE_ENV=production MODE_ENV=staging PORT=443 NODE_EXTRA_CA_CERTS=./secrets/fullchain.cert.pem node dist/start.js --expose-http2 ",
    "start:production": "cross-env NODE_ENV=production MODE_ENV=production PORT=443 NODE_EXTRA_CA_CERTS=./secrets/fullchain.cert.pem node dist/start.js --expose-http2 ",
    "lint": "eslint \"*/**/*.ts*\"",
    "eslint": "eslint . --ignore-path .gitignore",
    "eslint:fix": "eslint . --fix --ignore-path .gitignore",
    "gen:schema": "graphql-codegen"
  },
  "keywords": [],
  "author": "Vic Pil",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.1.4",
    "@apollo/link-context": "^2.0.0-beta.3",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@apollo/link-retry": "^2.0.0-beta.3",
    "@apollo/link-ws": "^2.0.0-beta.3",
    "@apollo/react-hooks": "^4.0.0",
    "@apollo/react-ssr": "^4.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@hapi/iron": "^6.0.0",
    "@zeit/next-sass": "^1.0.1",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-server": "^2.17.0",
    "apollo-server-express": "^2.17.0",
    "apollo-utilities": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie": "^0.4.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-passport": "^0.6.3",
    "graphql-resolvers": "^0.4.2",
    "graphql-tag": "^2.11.0",
    "helmet": "^4.1.0",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.5",
    "lodash.get": "^4.4.2",
    "next": "^9.5.2",
    "next-connect": "^0.8.1",
    "next-cookies": "^2.0.3",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.3.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.10",
    "subscriptions-transport-ws": "^0.9.18",
    "swr": "^0.3.2",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.13",
    "@types/js-cookie": "^2.2.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash.get": "^4.4.6",
    "@types/node": "^14.6.4",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^16.9.49",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-jest": "^26.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "faker": "^5.1.0",
    "jest": "^26.4.2",
    "jest-watch-typeahead": "^0.6.0",
    "mocha": "^8.1.3",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "styled-jsx-plugin-sass": "^1.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120,
    "semi": true,
    "tabWidth": 2,
    "endOfLine": "auto"
  }
}
