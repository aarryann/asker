{
  "presets": [["next/babel"]],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./components",
          "@clientconfig": "./config",
          "@serverconfig": "./server/config",
          "@lib": "./lib",
          "@mocks": "./__mocks__",
          "@pages": "./pages",
          "@public": "./public",
          "@tests": "./__tests__",
          "@theme": "./theme"
        }
      }
    ]
  ]
}
